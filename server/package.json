{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production nodemon build/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --files src/index.ts",
    "test": "cross-env NODE_ENV=test vitest --ui",
    "coverage": "cross-env NODE_ENV=test vitest run --coverage",
    "lint": "eslint .",
    "tsc": "tsc && npm run sentry:sourcemaps",
    "migration:down": "node src/util/rollback.js",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org self-01b --project node-express ./build && sentry-cli sourcemaps upload --org self-01b --project node-express ./build"
  },
  "dependencies": {
    "@sentry/cli": "^2.32.2",
    "@sentry/node": "^8.18.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "pg": "^8.12.0",
    "sequelize": "^6.37.3",
    "umzug": "^3.8.1",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@faker-js/faker": "^8.4.1",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.6",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "glob": "^10.4.2",
    "globals": "^15.6.0",
    "nodemon": "^3.1.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.13.1",
    "vitest": "^1.6.0"
  }
}